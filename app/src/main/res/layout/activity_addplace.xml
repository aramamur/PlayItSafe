<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/place_cleaning"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.addreview.AddPlaceActivity">

    <Button
        android:id="@+id/button_place"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Place"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_plexiglass"
        app:layout_constraintVertical_bias="0.097" />

    <CheckBox
        android:id="@+id/check_clean"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cleaning Procedures"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.478"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/display_text" />

    <CheckBox
        android:id="@+id/check_masks"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Masks Required"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_clean" />

    <CheckBox
        android:id="@+id/check_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Distancing and Aisle Ways"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_masks" />

    <CheckBox
        android:id="@+id/check_limitusers"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Limiting Users"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.489"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_distance" />

    <CheckBox
        android:id="@+id/check_cleancarts"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cleaning Carts"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.499"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_limitusers" />

    <CheckBox
        android:id="@+id/check_plexiglass"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Plexiglass for Employees"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/check_cleancarts" />

    <TextView
        android:id="@+id/display_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>